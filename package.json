{
  "name": "webapp-go",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "serve": "go run ./server",
    "p-watch": "parcel watch ./client/public/index.html --public-url /static/",
    "p-build": "parcel build ./client/public/index.html --public-url /static/",
